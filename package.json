{
  "name": "peap_demo",
  "version": "1.0.0",
  "description": "A poc implementation of Phoenix Elixir Angular Postgres and few other things.",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test-client-unit": "bin/run_client_unit_tests.sh",
    "test-server": "mix test",
    "test-e2e": "bin/run_e2e_tests.sh",
    "test": "bin/run_all_tests.sh",
    "clean": "rm -rf priv/static/*",
    "copy": "cp web/static/root/* priv/static",
    "copy-watch": "node_modules/.bin/cpx \"web/static/root/*\" priv/static --watch",
    "webpack": "node_modules/.bin/webpack",
    "webpack-watch": "node_modules/.bin/webpack --watch-stdin --progress --colors",
    "webpack-test": "node_modules/.bin/webpack --config webpack-test.config.js",
    "typings": "typings",
    "postinstall": "typings install",
    "start": "mix phoenix.server"
  },
  "author": "Adam Keating",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.1.0",
    "@angular/compiler": "2.1.0",
    "@angular/core": "2.1.0",
    "@angular/forms": "2.1.0",
    "@angular/http": "2.1.0",
    "@angular/platform-browser": "2.1.0",
    "@angular/platform-browser-dynamic": "2.1.0",
    "@angular/router": "3.1.0",
    "@types/jasmine": "^2.5.35",
    "bootstrap": "^3.3.6",
    "es6-shim": "0.35.1",
    "font-awesome": "4.6.3",
    "ng2-bootstrap": "^1.1.16",
    "phoenix": "1.2.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "webpack": "^2.1.0-beta.22",
    "zone.js": "0.6.25"
  },
  "devDependencies": {
    "awesome-typescript-loader": "2.2.4",
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-strict-mode": "6.11.3",
    "babel-preset-es2015": "6.16.0",
    "chai": "3.5.0",
    "cpx": "1.5.0",
    "css-loader": "0.25.0",
    "css-to-string-loader": "^0.1.2",
    "file-loader": "0.9.0",
    "globby": "6.0.0",
    "jasmine-core": "2.5.2",
    "karma": "1.3.0",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-jasmine": "1.0.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.8.0",
    "less": "2.7.1",
    "less-loader": "2.2.3",
    "node-sass": "^3.10.1",
    "protractor": "4.0.9",
    "raw-loader": "0.5.1",
    "resolve-url-loader": "1.6.0",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1",
    "typescript": "2.0.3",
    "typings": "1.4.0",
    "url-loader": "0.5.7",
    "val-loader": "0.5.0",
    "webpack": "^2.1.0-beta.22"
  }
